{
  "name": "eslint-plugin-lean-imports",
  "version": "0.1.0",
  "description": "ESLint Plugin to make sure your imports only import the bits you need",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "test": "npm run lint && mocha",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint ./",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eslint-plugins/eslint-plugin-lean-imports.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eslint-plugins/eslint-plugin-lean-imports/issues"
  },
  "homepage": "https://github.com/eslint-plugins/eslint-plugin-lean-imports#readme",
  "devDependencies": {
    "babel": "^5.8.5",
    "babel-eslint": "^3.1.26",
    "eslint": "^0.24.1",
    "eslint-tester": "^0.8.2",
    "is-my-json-valid": "^2.12.0",
    "mocha": "^2.2.5",
    "mt-changelog": "^0.6.1",
    "release-script": "^0.2.1"
  },
  "keywords": [
    "eslint-plugin",
    "eslintplugin",
    "eslint",
    "import",
    "lodash",
    "react-bootstrap"
  ]
}
